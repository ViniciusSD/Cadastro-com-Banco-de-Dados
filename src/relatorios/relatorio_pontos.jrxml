	<queryString>
		<![CDATA[SELECT 
    e.nome AS equipe,
    p.nome AS piloto_nome,
    p.numero AS piloto_numero,
    p.pontos AS piloto_pontos,
    total.total_pontos
FROM 
    pilotos p
JOIN 
    equipes e 
ON 
    LOWER(p.equipe) = LOWER(e.nome)
JOIN (
    SELECT 
        LOWER(equipe) AS equipe_normalizada,
        SUM(pontos) AS total_pontos
    FROM 
        pilotos
    GROUP BY 
        LOWER(equipe)
) total 
ON 
    LOWER(e.nome) = total.equipe_normalizada
ORDER BY 
    e.nome, p.nome;]]>
